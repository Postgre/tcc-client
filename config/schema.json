{
  "Market": {
    "endpoint": "markets",
    "required": [
      "published",
      "rate_caroler_base",
      "rate_caroler_discount",
      "rate_travel_distance",
      "rate_travel_duration",
      "name",
      "city",
      "phone",
      "email",
      "image",
      "html",
      "address",
      "state",
      "rate_caroler_first",
      "rate_caroler_second",
      "rate_caroler_third",
      "rate_caroler_fourth",
      "rate_caroler_fifth"
    ],
    "defaults": {
      "rate_caroler_first": 100,
      "rate_caroler_second": 100,
      "rate_caroler_third": 100,
      "rate_caroler_fourth": 100,
      "rate_caroler_fifth": 100
    }
  },
  "Reseller": {
    "endpoint": "resellers",
    "required": [
      "name",
      "address"
    ],
    "optional": [
      "promotion_id"
    ]
  },
  "Booking": {
    "endpoint": "events",
    "required": [
      "name",
      "market_id",
      "start_time",
      "end_time",
      "state",
      "city",
      "address",
      "type",
      "requests"
    ],
    "defaults": {
      "type": "personal",
      "caroler_config": "quartet"
    }
  },
  "PromoCode": {
    "endpoint": "promotions",
    "required": [
      "name",
      "description",
      "pricing_scale",
      "pricing_offset",
      "start_time",
      "end_time"
    ],
    "optional": [
      "code",
      "active"
    ],
    "defaults": {
      "pricing_scale": 1,
      "pricing_offset": 0
    }
  },
  "Resellers": {
    "endpoint": "resellers",
    "required": [
      "name",
      "address"
    ],
    "optional": [
      "code"
    ]
  },
  "User": {
    "endpoint": "users",
    "required": [
      "name",
      "email",
      "phone",
      "state",
      "city",
      "address",
      "image",
      "bio"
    ]
  }
}